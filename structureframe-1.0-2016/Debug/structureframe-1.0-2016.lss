                 	.ORG INT4addr	; External Interrupt Request 4
00000a 9518      	RETI
                 	.ORG INT5addr	; External Interrupt Request 5
00000c 9518      	RETI
                 	.ORG INT6addr	; External Interrupt Request 6
00000e 9518      	RETI
                 	.ORG INT7addr	; External Interrupt Request 7
000010 9518      	RETI
