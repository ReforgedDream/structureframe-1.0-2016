                 .equ segind8 = 0x80	//
                 .equ segind9 = 0x90	//
                 .equ segindDash = 0b_1011_1111		//"-" sign
                 .equ segindA = 0b_1000_1000		//"A"
                 .equ segindB = 0b_1000_0011		//"b"
                 .equ segindC = 0b_1100_0110		//"C"
                 .equ segindD = 0b_1010_0001		//"d"
