                 //150 Hz (0x01A1) works well for the LED indicator
                 //200 Hz (0x0139) for the green 4-digit indicator
                 .equ owfPerSecond8BitLow = 0x24
                 .equ owfPerSecond8BitHigh = 0xF4 //supra
                 
