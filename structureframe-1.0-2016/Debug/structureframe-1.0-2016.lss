00011d           spiMOSI:			.BYTE spiRAMStorageMOSILength	//...and MOSI buffers
000125           spiMOSIRead:		.BYTE 1
000126           spiMOSIWrite:		.BYTE 1		//read and write pointers
                 
000127           subrStartLabel:		.BYTE 2
